{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.html",
            "headers": {
                "Content-Type": "text/html"
            }
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        }
    ]
}